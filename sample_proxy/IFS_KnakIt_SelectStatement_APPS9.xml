<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="IFS_KnakIt_SelectStatement_APPS9"
       transports="http https"
       startOnLoad="true">
   <description/>
   <target>
      <inSequence>
         <property name="SqlStatement"
                   expression="$body//request/SqlStatement"
                   scope="default"
                   type="STRING"/>
         <knakitifs.selectStatement>
            <IfsConnURL>IFS_URL:PORT</IfsConnURL>
            <IfsVersion>APPS9</IfsVersion>
            <IfsUserID>USER</IfsUserID>
            <IfsPassword>USERPW</IfsPassword>
            <SqlStatement>{$ctx:SqlStatement}</SqlStatement>
         </knakitifs.selectStatement>
         <respond/>
      </inSequence>
   </target>
</proxy>
