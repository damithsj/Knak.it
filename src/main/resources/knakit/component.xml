<component name="knakit" type="synapse/template" >
    <subComponents> 
		<component name="selectStatement" >
	    		<file>selectStatement.xml</file>
	    		<description>Execute SQL statement and retrives records from IFS</description>
		</component>	
		<component name="plsqlBlock" >
	    		<file>plsqlBlock.xml</file>
	    		<description>Execute PL/SQL block and retrieves the values assigned for bind variables</description>
		</component>
		<component name="plsqlBaseMethod" >
	    		<file>plsqlBaseMethod.xml</file>
	    		<description>Execute PL/SQL base method for NEW, MODIFY, REMOVE</description>
		</component>		
    </subComponents>    
</component>